{
  "name": "ng2-toastr",
  "version": "6.0.1-20190530",
  "description": "bootstrap style of toast for modern angular",
  "main": "ng2-toastr.js",
  "typings": "ng2-toastr.d.ts",
  "scripts": {
    "typings": "typings install",
    "webpack": "webpack --progress --profile --bail",
    "build:prod": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "build:dev": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc",
    "lite": "lite-server",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "prepublish": "gulp clean && ngc && node make.js && gulp bundle-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wojciechpolak/ng2-toastr.git"
  },
  "keywords": [
    "toast",
    "toaster",
    "toastr",
    "angular 2",
    "bootstrap",
    "notification"
  ],
  "author": "Derek Shi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wojciechpolak/ng2-toastr/issues"
  },
  "homepage": "https://github.com/wojciechpolak/ng2-toastr#readme",
  "devDependencies": {
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/animations": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/platform-server": "6.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "bootstrap": "^3.3.6",
    "async": "2.6.0",
    "concurrently": "3.5.1",
    "css-loader": "0.28.11",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-cssnano": "2.1.3",
    "gulp-eslint": "4.0.2",
    "gulp-rename": "1.2.2",
    "gulp-rimraf": "~0.2",
    "gulp-size": "3.0.0",
    "gulp-tslint": "8.1.3",
    "lite-server": "^2.2.2",
    "require-dir": "1.0.0",
    "style-loader": "0.21.0",
    "systemjs": "0.21.3",
    "systemjs-builder": "0.16.13",
    "rxjs": "6.1.0",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "zone.js": "^0.8.4"
  },
  "dependencies": {}
}
